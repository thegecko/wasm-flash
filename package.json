{
  "name": "wasm-flash",
  "version": "1.0.0",
  "description": "Example of flashing using a WebAssembly module",
  "license": "MIT",
  "main": "dist/index.js",
  "author": "Rob Moran <github@thegecko.org>",
  "homepage": "https://github.com/thegecko/wasm-flash",
  "repository": {
    "type": "git",
    "url": "git://github.com/thegecko/wasm-flash.git"
  },
  "scripts": {
    "emcc": ". ~/emsdk/emsdk_env.sh && emcc ./emscripten/flash.cpp -o ./src/wasm/flash.js --js-library ./emscripten/merge-into.js --post-js ./emscripten/post.js -s EXTRA_EXPORTED_RUNTIME_METHODS=['ccall','UTF8ToString'] -s MODULARIZE=1 -s EXPORT_ES6=1 -s ASYNCIFY=1 -s ASYNCIFY_IMPORTS=['logMessage','usbOpen','usbClose','transferIn','transferOut']",
    "build": "npm run emcc && rollup -c",
    "watch": "npm run emcc && rollup -c rollup.dev.config.js -w"
  },
  "devDependencies": {
    "@types/emscripten": "^1.39.4",
    "@types/w3c-web-usb": "^1.0.4",
    "rollup": "^2.2.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-delete": "^1.2.0",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.2"
  }
}
